    "devDependencies": {
        "@next/bundle-analyzer": "^12.2.2",
        "@types/node": "^18.0.3",
        "@types/prettier": "^2.6.3",
        "@types/react": "^18.0.15",
        "@types/react-dom": "^18.0.6",
        "@typescript-eslint/eslint-plugin": "^5.30.5",
        "@typescript-eslint/parser": "^5.30.6",
        "cross-env": "^7.0.3",
